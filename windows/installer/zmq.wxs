<!--    To build the .msi file from zmq.wxs:                                        -->
<!--        1. Wix version 3.0 or later is needed                                   -->
<!--        2. candle.exe zmq.wxs UI_Texts.wxs                                      -->
<!--        the output from 2. is zmq.wixobj UI_Texts.wixobj                        -->
<!--        3. light.exe -out zmq_v1.0.0.msi zmq.wixobj UI_Texts.wixobj             -->
<!--           -ext WixNetFxExtension.dll -loc zmq.wxl                              --> 
<!--                                                                                -->
<!--    In one directory, you have to have these files to build .msi file:          -->
<!--        zmq.wxs UI_Texts.wxs                                                    -->
<!--        zmq.wxl                                                                 -->
<!--        logo-side.bmp   logo-top.bmp    zeromq.ico    Licence.rtf               -->
<!--        directories 'bin' 'examples' 'java' 'perf' 'python' 'ruby' 'doc'        -->
<!--           'include' 'lib' from 'tmp'                                           -->
<!--            directory that is created by 'pack' project in MSVC.                -->
<!--        files Changelog Authors COPYING COPYING.LESSER                          -->
<!--                                                                                -->

<Wix xmlns='http://schemas.microsoft.com/wix/2006/wi'>
 
    <!-- Basic information about the product -->
    <!-- When doing upgrade, the UpgradeCode stays the same, otherwise the upgrade will not be possible -->
    <!-- When doing upgrade, change the ProductCode and ProductVersion -->
    
    <?define ProductVersion="1.0.0" ?>
    <?define UpgradeCode="{326DC84C-5CCC-44F9-A632-6E10A2FC9E94}"?>
    <?define Manufacturer="ØMQ"?>
    <?define ProductCode="{43062DAE-EC10-474C-87F8-EA6CB7035AB9}"?>
    <?define PackageCode="*"?>

    <Product Name='ØMQ 1.0.0' Id='$(var.ProductCode)' UpgradeCode='$(var.UpgradeCode)'
        Language='1033' Codepage='1252' Version='$(var.ProductVersion)' Manufacturer='$(var.Manufacturer)'>
 
    <Package Keywords='Installer' Id='$(var.PackageCode)' Description="ZEROMQ messaging" Comments='This is ZEROMQ installer.' 
        Manufacturer='$(var.Manufacturer)' InstallerVersion='100' Languages='1033' Compressed='yes' SummaryCodepage='1252' />
       
    <!-- Sets the ZEROMQ icon that can be seen in Add/Remove programs dialog. -->
    <!-- It is not possible to change the installer (.msi) icon using WiX. -->
    <Icon Id='zmq_v1.0.0.exe' SourceFile='zeromq.ico'/>
    <Property Id="ARPPRODUCTICON" Value="zmq_v1.0.0.exe" />
    <Property Id="ALLUSERS" Value="1"/>
        
    <!-- Before installing, checks if previous versions of ZEROMQ are installed and removes them. -->    
    <InstallExecuteSequence>
        <RemoveExistingProducts After='InstallInitialize' />
    </InstallExecuteSequence>

    <!-- To set ZEROMQ logo and licence. The files Licence.rtf logo-top.bmp logo-side.bmp -->
    <!-- have to be present in the same directory as zmq.wxs. -->
    <WixVariable Id="WixUILicenseRtf" Value="Licence.rtf" />
    <WixVariable Id="WixUIBannerBmp" Value="logo-top.bmp" />
    <WixVariable Id="WixUIDialogBmp" Value="logo-side.bmp" />  
    
    
    <Media Id='1' Cabinet='Sample.cab' EmbedCab='yes' DiskPrompt="CD-ROM #1" />
 	<Property Id='DiskPrompt' Value="ØMQ installation [1]" />
    
 	<!--  Search for dependencies in registry. -->
 	<Property Id="MSMQ">
         <RegistrySearch Id="MSMQVersion" Root="HKLM" Key="SOFTWARE\Microsoft\MSMQ\Parameters" Type="raw" Name="CurrentBuild" />
    </Property>
        
    <Property Id="PYTHON">
        <RegistrySearch Id="PYTHONVersion" Root="HKLM" Key="SOFTWARE\Python\PythonCore\2.6\InstallPath" Type="raw" /> 
    </Property>
    
     <Property Id="JAVA">
        <RegistrySearch Id="JavaVersion" Root="HKLM" Key="SOFTWARE\JavaSoft\Java Runtime Environment" Type="raw" Name="CurrentVersion" />
    </Property>
    
    <Property Id="DOTNET">
         <![CDATA[NETFRAMEWORK20 OR NETFRAMEWORK30 OR NETFRAMEWORK35]]>
    </Property>
      
    <Property Id="DialogBitmapTop">dlgbmptop</Property>
    <Property Id="DialogBitmapSide">dlgbmpside</Property>
    
    <!--  Set the font in custom dialog. -->
    <Property Id="DlgTitleFont">{&amp;DlgFontBold8}</Property>
    <Property Id="DlgErrorFont">{&amp;DlgFontBold10}</Property>
    <Property Id="DlgFont">{&amp;DlgFont8}</Property>

    <!--  Set logo in custom dialog. -->
    <Binary Id="dlgbmptop" SourceFile="logo-top.bmp" />
    <Binary Id="dlgbmpside" SourceFile="logo-side.bmp" />
    
